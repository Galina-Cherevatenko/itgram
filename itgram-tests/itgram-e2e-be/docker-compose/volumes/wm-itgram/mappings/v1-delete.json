{
  "request": {
    "method": "POST",
    "url": "/v1/publication/delete"
  },
  "response": {
    "status": 200,
    "jsonBody": {
      "responseType": "delete",
      "result": "success",
      "ad": {
        "id": "{{{jsonPath request.body '$.publication.id'}}}",
        "title": "title",
        "description": "decription",
        "publicationCategory": "post",
        "visibility": "public"
      }
    },
    "headers": {
      "Content-Type": "application/json"
    },
    "transformers": ["response-template"]
  }
}
